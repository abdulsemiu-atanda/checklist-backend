{
  "name": "checklist-backend",
  "version": "1.0.0",
  "description": "Todo App that helps users add, create and share todo items",
  "directories": {
    "test": "tests"
  },
  "main": "index.js",
  "scripts": {
    "build": "yarn babel src --out-dir dist --source-maps inline --ignore 'src/db/migrations/*','src/db/seeders/*'",
    "clean": "rm -rf dist/",
    "pretest": "NODE_ENV=test yarn sequelize-cli db:migrate",
    "dev": "NODE_ENV=development nodemon ./src/index.js --exec babel-node",
    "start": "node dist/index.js",
    "test": "NODE_ENV=test babel-node node_modules/.bin/nyc mocha",
    "prepare:db": "yarn sequelize-cli db:migrate && yarn sequelize-cli db:seed:all",
    "clean:db": "yarn sequelize-cli db:seed:undo:all && yarn sequelize-cli db:migrate:undo:all",
    "setup:admin": "node dist/setup/adminSetup.js",
    "setup:admin:dev": "yarn babel-node src/setup/adminSetup.js"
  },
  "repository": "https://github.com/abdulsemiu-atanda/checklist-backend.git",
  "author": "Abdulsemiu Atanda <semiu@live.com>",
  "license": "MIT",
  "private": true,
  "dependencies": {
    "@babel/preset-env": "^7.26.0",
    "@babel/register": "^7.25.9",
    "bcrypt": "^5.1.1",
    "body-parser": "^1.20.3",
    "dotenv": "^16.4.5",
    "express": "^4.21.1",
    "graceful": "^1.1.0",
    "inquirer": "^12.1.0",
    "jsonwebtoken": "^9.0.2",
    "marked": "^15.0.2",
    "morgan": "^1.10.0",
    "pg": "^8.13.1",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.37.5",
    "uuid": "^11.0.2",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.25.9",
    "@babel/core": "^7.26.0",
    "@babel/node": "^7.26.0",
    "@eslint/js": "^9.15.0",
    "@faker-js/faker": "^9.2.0",
    "chai": "^4.5.0",
    "cli-table": "^0.3.11",
    "coveralls": "^3.1.1",
    "eslint": "^9.15.0",
    "express-list-endpoints": "^7.1.1",
    "globals": "^15.12.0",
    "http-proxy-middleware": "^2.0.7",
    "maildev": "^2.1.0",
    "mocha": "^10.8.2",
    "nodemon": "^3.1.7",
    "nyc": "^17.1.0",
    "sequelize-cli": "^6.6.2",
    "supertest": "^7.0.0",
    "tweetnacl-util": "^0.15.1"
  },
  "engines": {
    "node": ">=20.9.0",
    "npm": ">=10.1.0"
  }
}
